{
  "name": "logitar-vue3-ui",
  "version": "0.1.0",
  "description": "Helper functions & Bootstrap5 component library for Vue3 distributed by Logitar.",
  "keywords": [
    "logitar",
    "vue",
    "vue3",
    "bootstrap",
    "bootstrap5",
    "bootstrap53",
    "fontawesome",
    "helper",
    "functions",
    "component",
    "library",
    "checkbox"
  ],
  "homepage": "https://www.francispion.ca/",
  "bugs": {
    "url": "https://github.com/Logitar/Vue3Ui/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Francis Pion",
    "email": "francispion@hotmail.com",
    "url": "https://www.francispion.ca/"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/logitar-vue3-ui.umd.cjs",
  "module": "./dist/logitar-vue3-ui.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/logitar-vue3-ui.js",
      "require": "./dist/logitar-vue3-ui.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Logitar/Vue3Ui"
  },
  "type": "module",
  "packageManager": "yarn@4.0.2",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "test:dev": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "nanoid": "^5.0.4"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.5",
    "@popperjs/core": "^2.11.8",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.10.5",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitest/coverage-v8": "^1.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.0",
    "bootstrap": "^5.3.2",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^23.0.1",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.6.4",
    "vitest": "^1.0.4",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.25"
  },
  "peerDependencies": {
    "vue": "^3.3.11"
  }
}
