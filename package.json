{
  "name": "logitar-vue3-ui",
  "version": "2.3.0",
  "description": "Helper functions & Bootstrap5 component library for Vue3 distributed by Logitar.",
  "keywords": [
    "logitar",
    "vue",
    "vue3",
    "bootstrap",
    "bootstrap5",
    "bootstrap53",
    "fontawesome",
    "helper",
    "functions",
    "component",
    "library",
    "accordion",
    "alert",
    "avatar",
    "badge",
    "breadcrumb",
    "button",
    "card",
    "carousel",
    "checkbox",
    "image",
    "input",
    "modal",
    "pagination",
    "progress",
    "select",
    "spinner",
    "tab",
    "tabs",
    "textarea",
    "toast",
    "toasts",
    "tooltip"
  ],
  "homepage": "https://www.francispion.ca/",
  "bugs": {
    "url": "https://github.com/Logitar/Vue3Ui/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Francis Pion",
    "email": "francispion@hotmail.com",
    "url": "https://www.francispion.ca/"
  },
  "files": [
    "dist/**/*.cjs",
    "dist/**/*.css",
    "dist/**/*.d.ts",
    "dist/**/*.js",
    "dist/**/*.map"
  ],
  "main": "./dist/logitar-vue3-ui.umd.cjs",
  "module": "./dist/logitar-vue3-ui.js",
  "style": "./dist/style.css",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/logitar-vue3-ui.js",
      "require": "./dist/logitar-vue3-ui.umd.cjs",
      "types": "./dist/index.d.ts"
    },
    "./dist/**/*.css": {
      "import": "./dist/**/*.css",
      "require": "./dist/**/*.css"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Logitar/Vue3Ui"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "test": "vitest run --coverage",
    "test:dev": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "md5": "^2.3.0",
    "nanoid": "^5.0.4"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.3",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.5",
    "@popperjs/core": "^2.11.8",
    "@rushstack/eslint-patch": "^1.3.3",
    "@storybook/addon-essentials": "^8.0.8",
    "@storybook/addon-interactions": "^8.0.8",
    "@storybook/addon-links": "^8.0.8",
    "@storybook/blocks": "^8.0.8",
    "@storybook/test": "^8.0.8",
    "@storybook/vue3": "^8.0.8",
    "@storybook/vue3-vite": "^8.0.8",
    "@tsconfig/node20": "^20.1.2",
    "@types/bootstrap": "^5.2.10",
    "@types/jsdom": "^21.1.6",
    "@types/md5": "^2",
    "@types/node": "^20.10.5",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitest/coverage-v8": "^1.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.0",
    "bootstrap": "^5.3.2",
    "eslint": "^8.49.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^23.0.1",
    "npm-run-all2": "^6.1.1",
    "pinia": "^2.1.7",
    "prettier": "^3.0.3",
    "storybook": "^8.0.8",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.6.4",
    "vitest": "^1.0.4",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.25"
  },
  "peerDependencies": {
    "vue": "^3.3.11"
  }
}
